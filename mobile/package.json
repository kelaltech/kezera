{
  "name": "SpvaMobile",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "run-s": "run-s",
    "run-p": "run-p",
    "prod": "run-s format tsc",
    "clean-android": "cd android && gradlew clean",
    "apk": "set ENVFILE=.env.staging&& run-s clean-android && cd android && gradlew assembleRelease",
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant release",
    "ios": "react-native run-ios",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "tsc": "tsc",
    "lint": "prettier {.,**}/*.{tsx,ts,jsx,js,json,md,scss,sass,css,less}",
    "format": "prettier --write {.,**}/*.{tsx,ts,jsx,js,json,md,scss,sass,css,less}"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.3.4",
    "axios": "^0.18.0",
    "i18next": "^15.1.0",
    "qs": "^6.7.0",
    "react": "16.8.6",
    "react-i18next": "^10.9.0",
    "react-native": "0.59.5",
    "react-native-config": "^0.11.7",
    "react-native-elements": "^1.1.0",
    "react-native-gesture-handler": "^1.2.1",
    "react-native-vector-icons": "^6.4.2",
    "react-native-webview": "^5.8.1",
    "react-navigation": "^3.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@types/i18next": "^12.1.0",
    "@types/jest": "^24.0.12",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.16",
    "@types/react-native": "^0.57.51",
    "@types/react-native-vector-icons": "^6.4.0",
    "@types/react-navigation": "^3.0.6",
    "@types/react-test-renderer": "^16.8.1",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.8.6",
    "typescript": "^3.4.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
